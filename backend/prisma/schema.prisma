generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UdyamFormData {
  id                Int       @id @default(autoincrement())
  aadhaar_no        String   @db.VarChar(12) @unique
  entrepreneur_name String   @db.VarChar(255)
  otp               String   @db.VarChar(6)
  org_type          String   @db.VarChar(100)
  pan_number        String   @db.VarChar(10) @unique
  pan_holder_name   String   @db.VarChar(255)
  dob               DateTime @db.Date
  created_at        DateTime @default(now()) @db.Timestamp(6)

  @@map("udyam_form_data")
}
